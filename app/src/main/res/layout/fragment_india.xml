<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.india.view.IndiaFragment">

    <TextView
        android:layout_margin="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="Ministry of Health Whatsapp Contact "
        android:textSize="16dp"
        android:id="@+id/tv_moh_india_whatsapp_lable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:layout_margin="32dp"
        android:clickable="true"
        android:autoLink="web"
        app:layout_constraintTop_toBottomOf="@+id/tv_moh_india_whatsapp_lable"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/india_whatsapp_url"
        android:id="@+id/tv_moh_india_whatsapp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</androidx.constraintlayout.widget.ConstraintLayout>